IKRS.RegexUnion = function( left, right ) {

    IKRS.Object.call( this );

    this.left  = left;
    this.right = right;
};

IKRS.RegexUnion.prototype.match = function( reader ) {
    // ...
};

IKRS.RegexUnion.prototype.toString = function() {
    return "(" + this.left + "|" + this.right + ")";
}
